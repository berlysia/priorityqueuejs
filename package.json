{
  "name": "priorityqueue",
  "version": "0.2.1",
  "description": "An implementation of Priority Queue",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib -w",
    "lint": "eslint .",
    "prepublish": "npm run clean && npm run build",
    "clean": "rm -rf lib/*",
    "test": "jest",
    "precommit": "lint-staged && npm test"
  },
  "author": "berlysia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:berlysia/priorityqueuejs.git"
  },
  "keywords": [
    "priority",
    "queue",
    "priority queue",
    "heap",
    "data structure"
  ],
  "devDependencies": {
    "@berlysia/eslint-config": "^0.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "flow-bin": "^0.66.0",
    "husky": "^0.14.3",
    "jest": "^22.4.0",
    "lint-staged": "^7.0.0",
    "regenerator-runtime": "^0.11.1"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coverageDirectory": "<rootDir>/coverage"
  }
}
