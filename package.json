{
  "name": "priorityqueue",
  "version": "1.0.0-rc1",
  "description": "An implementation of Priority Queue",
  "main": "lib/cjs",
  "module": "lib/esm",
  "sideEffect": false,
  "scripts": {
    "build:cjs": "cross-env NODE_ENV=commonjs babel src --extensions .ts --ignore **/*.test.ts,**/testing/**/*,**/performance/**/* --out-dir lib/cjs",
    "build:esm": "cross-env NODE_ENV=esmodule babel src --extensions .ts --ignore **/*.test.ts,**/testing/**/*,**/performance/**/* --out-dir lib/esm",
    "build:type:cjs": "tsc -p tsconfig.json --emitDeclarationOnly --outDir lib/cjs",
    "build:type:esm": "tsc -p tsconfig.json --emitDeclarationOnly --outDir lib/esm",
    "build": "run-s clean build:* build:type:*",
    "clean:perf": "rm -rf perf_results/*",
    "perf:build": "cross-env NODE_ENV=commonjs babel src --out-dir lib/cjs --extensions .ts",
    "perf:exec": "node lib/cjs/performance",
    "perf": "run-s clean perf:build perf:exec",
    "watch": "run-p \"build:* -- --watch\"",
    "lint": "eslint \"src/**\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "run-s clean build",
    "clean": "rm -rf lib/*",
    "test": "cross-env NODE_ENV=test jest",
    "changelog": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file --release-count 1 --append",
    "preversion": "run-s lint test typecheck build",
    "version": "run-s changelog && git add CHANGELOG.md",
    "ci": "run-s lint typecheck test",
    "postci": "codecov",
    "documentation": "documentation build src -f md > API.md"
  },
  "author": "berlysia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:berlysia/priorityqueuejs.git"
  },
  "keywords": [
    "priority",
    "queue",
    "priority queue",
    "heap",
    "data structure"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@berlysia/eslint-config": "^0.6.1",
    "@berlysia/tsconfig": "^0.0.1",
    "@types/babel__core": "^7.1.2",
    "@types/eslint": "^4.16.6",
    "@types/jest": "^24.0.15",
    "@types/lodash.range": "^3.2.6",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.6.2",
    "@types/prettier": "^1.16.4",
    "@types/seed-random": "^2.2.0",
    "@types/shuffle-array": "^0.0.28",
    "@types/yargs": "^13.0.0",
    "codecov": "^3.5.0",
    "conventional-changelog-cli": "^2.0.21",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "lodash.range": "^3.2.0",
    "microseconds": "^0.1.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "pkg-dir": "^4.2.0",
    "prettier": "^1.18.2",
    "seed-random": "^2.2.0",
    "shuffle-array": "^1.0.1",
    "simple-statistics": "^7.0.5",
    "typescript": "^3.5.3",
    "yargs": "^13.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!**/testing/**",
      "!**/performance/**"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib"
    ]
  },
  "files": [
    "lib"
  ]
}
